@startuml CLASS_command_def

title Command Pattern

interface Comando {
    +Response ejecutar(Request)
}
class Cliente{
    +Receptor receptor
    +Invocador invocador
}
class Receptor{
    +actuar()
}
class Invocador{
    +Comando comando
}

class ComandoImpl
Cliente *--> Invocador
Invocador *--> Comando
ComandoImpl ..|> Comando
ComandoImpl *--> Receptor
Cliente *--> Receptor
@enduml